public class SequentiaEx 
{
	public void show()
	{
		for(int i=1;i<=5;i++)
		{
			try
		   {
				Thread.sleep(200);
	    	}
		catch(InterruptedException e)
		{
					System.out.println(e);
		}
		System.out.println(i);
	}
	}
	public static void main(String[] args) 
	{
		SequentiaEx t1=new SequentiaEx ();
		SequentiaEx t2=new SequentiaEx ();
		SequentiaEx t3=new SequentiaEx ();
		t1.show();
		t2.show();
		t3.show();
	}

}
